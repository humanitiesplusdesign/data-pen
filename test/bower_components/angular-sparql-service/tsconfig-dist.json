{
    "compilerOptions": {
        "module": "es6",
        "moduleResolution": "node",
        "target": "es5",
        "rootDir": "src",
        "outDir": "dist",
        "declaration": true
    },
    "include": [
        "src/**/*.ts"
    ],
    "filesGlob": [
        "src/**/*.ts",
        "node_modules/@types/**/*.d.ts"
    ],
    "files": [
        "src/sparql-service.ts",
        "node_modules/@types/angular/index.d.ts",
        "node_modules/@types/jquery/index.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
