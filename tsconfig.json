{
    "compilerOptions": {
        "module": "amd",
        "target": "es5",
        "rootDir": "app",
        "outDir": ".tmp"
    },
    "filesGlob": [
        "app/scripts/**/*.ts",
        "app/bower_components/**/*.d.ts",
        "!app/bower_components/**/node_modules/**/*.ts",
        "!app/bower_components/**/typings/**/*.ts",
        "!app/bower_components/angular-ui-router/api/angular-ui-router.d.ts",
        "typings/globals/**/*.d.ts"
    ],
    "files": [
        "app/scripts/app.ts",
        "app/scripts/sparql-autocomplete-component.ts",
        "app/scripts/sparql-autocomplete-service.ts",
        "app/scripts/configuration-service.ts",
        "app/scripts/configurations-component.ts",
        "app/scripts/author-component.ts",
        "app/scripts/sparql-item-component.ts",
        "app/scripts/sparql-item-service.ts",
        "app/scripts/sparql-tree-service.ts",
        "app/scripts/tree-component.ts",
        "app/bower_components/angular-sparql-service/dist/sparql-service.d.ts",
        "typings/globals/angular-http-auth/index.d.ts",
        "typings/globals/angular-ui-router/index.d.ts",
        "typings/globals/angular/index.d.ts",
        "typings/globals/jquery/index.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
