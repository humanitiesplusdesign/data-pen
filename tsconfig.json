{
    "compilerOptions": {
        "module": "es6",
        "moduleResolution": "node",
        "target": "es5",
        "rootDir": "app",
        "outDir": ".tmp",
        "strictNullChecks": true
    },
    "filesGlob": [
        "app/scripts/**/*.ts",
        "app/bower_components/**/*.d.ts",
        "node_modules/@types/**/*.d.ts",
        "node_modules/d3-v4-definitelytyped/src/d3/*.d.ts"
    ],
    "files": [
        "app/scripts/app-configuration-common.ts",
        "app/scripts/app-configuration-ui.ts",
        "app/scripts/app-configuration-worker.ts",
        "app/scripts/author-component.ts",
        "app/scripts/collection-utils.ts",
        "app/scripts/configuration-service.ts",
        "app/scripts/explore-component.ts",
        "app/scripts/fibra-service.ts",
        "app/scripts/rdf.ts",
        "app/scripts/rdfjs.ts",
        "app/scripts/sparql-autocomplete-component.ts",
        "app/scripts/sparql-autocomplete-service.ts",
        "app/scripts/sparql-item-component.ts",
        "app/scripts/sparql-item-service.ts",
        "app/scripts/sparql-tree-service.ts",
        "app/scripts/sparql-update-service.ts",
        "app/scripts/tree-component.ts",
        "app/scripts/type-select-component.ts",
        "app/scripts/worker-service.ts",
        "app/bower_components/angular-sparql-service/dist/sparql-service.d.ts",
        "node_modules/@types/angular-http-auth/index.d.ts",
        "node_modules/@types/angular-ui-router/index.d.ts",
        "node_modules/@types/angular/index.d.ts",
        "node_modules/@types/geojson/index.d.ts",
        "node_modules/@types/jquery/index.d.ts",
        "node_modules/d3-v4-definitelytyped/src/d3/index.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
