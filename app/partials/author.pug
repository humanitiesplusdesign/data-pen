.container-fluid.main
  .row.main.no-gutter
    #left-column.col-md-3
      h4 Overview
      tree(tree="$ctrl.classTree",on-select="$ctrl.alterSelection(treeNode)")
    #middle-column.col-md-4
      h4 Filter
      sparql-autocomplete(limit="30",by="group",on-select="$ctrl.selectItem(result)")
    #right-column.col-md-5
      h4 Item
      sparql-item(item-id="$ctrl.itemId",on-select="$ctrl.selectItem(value)")
